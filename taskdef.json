{
    "family": "spring-boot-docker",
    "containerDefinitions": [
        {
            "image": "%REPOSITORY_URI%:%BUILD_VERSION%",
            "name": "spring-boot-docker",
            "cpu": 10,
            "memory": 300,
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 8080,
                    "hostPort": 8080
                }
            ]
        }
    ]
}
